{
  "name": "@lamnhan/unistylus",
  "version": "0.0.0",
  "description": "An SCSS/CSS theming system.",
  "author": "Lam Nhan",
  "homepage": "https://lamnhan.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "css": "sass ./projects/unistylus/src/scss:./dist/unistylus/css",
    "build": "ng build unistylus --prod && npm run css && cpy README.md LICENSE dist/unistylus",
    "deploy": "cd ./dist/unistylus && npm publish && cd ../../",
    "docs": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "coverage": "cat ./coverage/*/lcov.info | coveralls",
    "webdriver-update-ci": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.37"
  },
  "keywords": [
    "css",
    "scss",
    "sass",
    "theming",
    "angular"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lamnhan/unistylus.git"
  },
  "bugs": {
    "url": "https://github.com/lamnhan/unistylus/issues"
  },
  "dependencies": {
    "@angular/animations": "~11.0.9",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/forms": "~11.0.9",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/router": "~11.0.9",
    "highlight.js": "^10.6.0",
    "ngx-quicklink": "^0.2.7",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular/cli": "~11.0.7",
    "@angular/compiler-cli": "~11.0.9",
    "@types/highlight.js": "^10.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "clean-css-cli": "^4.3.0",
    "codelyzer": "^6.0.0",
    "coveralls": "^3.1.0",
    "cpy-cli": "^3.1.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
